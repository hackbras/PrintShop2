        div(id"showWidthProduct")
          label(for="WidthProduct") Largura
          br
          input(id="WidthProduct" type="text" placeholder="0" onchange="resizeWidthProduct(this.value)")
        div(class"displayPanel")
          div(id="product" class="display_paisaje")
            span(id="productBack" class="cardBack")
              h1(class="product_titleBack") Título Trazeiro
              p(class="product_textBack") Texto trazeiro
            span(id="productFace" class="cardFront")
              h1(class="product_titleFront") Título Frontal
              p(class="product_textFront") Texto Frontal
        div(id="showHeightProduct")
          label(for="heightProduct") Altura
          br
          inputi(id="heightProduct" type="text" placeholder="0" onchange="resizeHeightProduct(this.value)")
    footer
      details
        summary Copyright 2017
