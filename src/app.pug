            ul
              li
                a(href="#") Imagem de fundo
              li
                a(href="#") Imagem
          li
            a(href="#") Formatar
            ul
              li(id="toolsFont")
                a(href="#") Fonte
                ul(id="menuFont")
                  li
                    a(href="#") Tipo
                  li
                    a(href="#") Tamanho
                  li
                    a(href="#") Negrito
                  li
                    a(href="#") Itálico
                  li
                    a(href="#") Sublinhado
              li
                a(href="#") Coluna
