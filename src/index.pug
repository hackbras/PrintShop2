        #showWidthProduct
          label(for="WidthProduct") Largura
          br
          input#WidthProduct(type="text" placeholder="0" onchange="resizeWidthProduct(this.value)")
        .displayPanel
          #product.display_paisaje
            span#productBack.cardBack
              h1.product_titleBack Título Trazeiro
              p.product_textBack Texto trazeiro
            span#productFace.cardFront
              h1.product_titleFront Título Frontal
              p.product_textFront Texto Frontal
